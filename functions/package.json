{
  "name": "nx-ng-starter-server",
  "private": true,
  "description": "Cloud Functions for Firebase",
  "main": "main.js",
  "scripts": {
    "deploy": "firebase deploy --only functions",
    "dist:clean": "rm -f ./main.js ./main.js.map ./*.txt; rm -rf ./proto",
    "dist:get": "npm run dist:clear; cp -r ../dist/apps/api/* ./; cp -r ../tools/proto ./proto",
    "logs": "firebase functions:log",
    "serve": "firebase serve --only functions",
    "shell": "firebase experimental:functions:shell",
    "start": "npm run shell",
    "start:emulators": "firebase emulators:start --only functions"
  },
  "dependencies": {
    "@grpc/grpc-js": "1.3.2",
    "@grpc/proto-loader": "0.6.2",
    "@nestjs/common": "7.6.17",
    "@nestjs/core": "7.6.17",
    "@nestjs/graphql": "7.10.6",
    "@nestjs/jwt": "7.2.0",
    "@nestjs/microservices": "7.6.17",
    "@nestjs/passport": "7.1.5",
    "@nestjs/platform-express": "7.6.17",
    "@nestjs/platform-ws": "7.6.17",
    "@nestjs/websockets": "7.6.17",
    "apollo-server-express": "2.24.1",
    "class-transformer": "0.4.0",
    "class-validator": "0.13.1",
    "dotenv": "9.0.2",
    "express": "4.17.1",
    "firebase-admin": "9.8.0",
    "firebase-functions": "3.14.1",
    "google-protobuf": "3.17.0",
    "graphql": "15.5.0",
    "graphql-subscriptions": "1.2.1",
    "graphql-tag": "2.12.4",
    "graphql-tools": "7.0.5",
    "graphql-upload": "12.0.0",
    "grpc-web": "1.2.1",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.0.1"
  },
  "engines": {
    "node": "10",
    "npm": ">=6.14.4"
  }
}
