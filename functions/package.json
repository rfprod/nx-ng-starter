{
  "name": "nx-ng-starter-server",
  "private": true,
  "description": "Cloud Functions for Firebase",
  "main": "main.js",
  "scripts": {
    "deploy": "firebase deploy --only functions",
    "dist:clean": "rm -rf ./main.js ./main.js.map ./*.txt ./proto ./schema.gql",
    "dist:get": "npm run dist:clear; cp -r ../dist/apps/api/* ./; mkdir ./proto; cp -r ../tools/proto/* ./proto ; cp ../libs/backend-gql/schema.gql ./",
    "logs": "firebase functions:log",
    "serve": "firebase serve --only functions",
    "shell": "firebase experimental:functions:shell",
    "start": "npm run shell",
    "start:emulators": "firebase emulators:start --only functions"
  },
  "dependencies": {
    "@grpc/grpc-js": "1.6.7",
    "@grpc/proto-loader": "0.6.12",
    "@nestjs/axios": "0.0.7",
    "@nestjs/common": "8.4.4",
    "@nestjs/apollo": "10.0.11",
    "@nestjs/core": "8.4.4",
    "@nestjs/graphql": "10.0.11",
    "@nestjs/jwt": "8.0.0",
    "@nestjs/microservices": "8.4.4",
    "@nestjs/passport": "8.2.1",
    "@nestjs/platform-express": "8.4.4",
    "@nestjs/platform-ws": "8.4.4",
    "@nestjs/websockets": "8.4.4",
    "@types/compression": "1.7.2",
    "@types/express": "4.17.13",
    "@types/graphql-upload": "8.0.11",
    "@types/node": "17.0.33",
    "@types/websocket": "1.0.5",
    "@types/ws": "8.5.3",
    "apollo-server-express": "3.7.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.13.2",
    "compression": "1.7.4",
    "dotenv": "16.0.1",
    "express": "4.18.1",
    "firebase-admin": "10.2.0",
    "firebase-functions": "3.21.1",
    "google-protobuf": "3.20.1",
    "graphql": "16.5.0",
    "graphql-subscriptions": "2.0.0",
    "graphql-tag": "2.12.6",
    "graphql-tools": "8.2.9",
    "graphql-upload": "13.0.0",
    "grpc-web": "1.3.1",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.5.5",
    "tslib": "2.4.0"
  },
  "engines": {
    "node": "10",
    "npm": ">=6.14.4"
  }
}
