{
  "name": "nx-ng-starter-server",
  "private": true,
  "description": "Cloud Functions for Firebase",
  "main": "main.js",
  "scripts": {
    "deploy": "firebase deploy --only functions",
    "dist:clean": "rm -rf ./main.js ./main.js.map ./*.txt ./proto ./schema.gql",
    "dist:get": "npm run dist:clear; cp -r ../dist/apps/api/* ./; mkdir ./proto; cp -r ../tools/proto/* ./proto ; cp ../libs/backend-gql/schema.gql ./",
    "logs": "firebase functions:log",
    "serve": "firebase serve --only functions",
    "shell": "firebase experimental:functions:shell",
    "start": "npm run shell",
    "start:emulators": "firebase emulators:start --only functions"
  },
  "dependencies": {
    "@grpc/grpc-js": "1.8.14",
    "@grpc/proto-loader": "0.7.7",
    "@nestjs/apollo": "11.0.5",
    "@nestjs/axios": "2.0.0",
    "@nestjs/common": "9.4.1",
    "@nestjs/config": "2.3.2",
    "@nestjs/core": "9.4.1",
    "@nestjs/graphql": "11.0.5",
    "@nestjs/jwt": "10.0.3",
    "@nestjs/microservices": "9.4.1",
    "@nestjs/passport": "9.0.3",
    "@nestjs/platform-express": "9.4.1",
    "@nestjs/platform-ws": "9.4.1",
    "@nestjs/swagger": "6.3.0",
    "@nestjs/websockets": "9.4.1",
    "@types/compression": "1.7.2",
    "@types/express": "4.17.17",
    "@types/graphql-upload": "16.0.0",
    "@types/node": "20.1.7",
    "@types/websocket": "1.0.5",
    "@types/ws": "8.5.4",
    "@apollo/server": "4.7.1",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "compression": "1.7.4",
    "dotenv": "16.0.3",
    "express": "4.18.2",
    "firebase-admin": "11.8.0",
    "firebase-functions": "4.4.0",
    "google-protobuf": "3.21.2",
    "graphql": "16.6.0",
    "graphql-subscriptions": "2.0.0",
    "graphql-tag": "2.12.6",
    "graphql-tools": "8.3.20",
    "graphql-upload": "16.0.2",
    "grpc-web": "1.4.2",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.8.1",
    "tslib": "2.5.0"
  },
  "engines": {
    "node": "10",
    "npm": ">=6.14.4"
  }
}
