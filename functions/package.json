{
  "name": "nx-ng-starter-server",
  "private": true,
  "description": "Cloud Functions for Firebase",
  "main": "main.js",
  "scripts": {
    "deploy": "firebase deploy --only functions",
    "dist:clean": "rm -rf ./main.js ./main.js.map ./*.txt ./proto ./schema.gql",
    "dist:get": "npm run dist:clear; cp -r ../dist/apps/api/* ./; mkdir ./proto; cp -r ../tools/proto/* ./proto ; cp ../libs/backend-gql/schema.gql ./",
    "logs": "firebase functions:log",
    "serve": "firebase serve --only functions",
    "shell": "firebase experimental:functions:shell",
    "start": "npm run shell",
    "start:emulators": "firebase emulators:start --only functions"
  },
  "dependencies": {
    "@grpc/grpc-js": "1.12.5",
    "@grpc/proto-loader": "0.7.13",
    "@nestjs/apollo": "12.2.2",
    "@nestjs/throttler": "6.3.0",
    "@nestjs/axios": "3.1.3",
    "@nestjs/common": "10.4.15",
    "@nestjs/config": "3.3.0",
    "@nestjs/core": "10.4.15",
    "@nestjs/graphql": "12.2.2",
    "@nestjs/jwt": "10.2.0",
    "@nestjs/microservices": "10.4.15",
    "@nestjs/passport": "10.0.3",
    "@nestjs/platform-express": "10.4.15",
    "@nestjs/platform-ws": "10.4.15",
    "@nestjs/swagger": "8.1.1",
    "@nestjs/websockets": "10.4.15",
    "@nestjs/cache-manager": "2.3.0",
    "cache-manager": "5.7.6",
    "@types/compression": "1.7.5",
    "@types/express": "5.0.0",
    "@types/graphql-upload": "17.0.0",
    "@types/node": "22.10.5",
    "@types/websocket": "1.0.10",
    "@types/ws": "8.5.13",
    "@apollo/server": "4.11.3",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.1",
    "compression": "1.7.5",
    "dotenv": "16.4.7",
    "express": "4.21.2",
    "firebase-admin": "12.5.0",
    "firebase-functions": "6.2.0",
    "google-protobuf": "3.21.4",
    "graphql": "16.10.0",
    "graphql-subscriptions": "3.0.0",
    "graphql-tag": "2.12.6",
    "graphql-tools": "9.0.11",
    "graphql-upload": "17.0.0",
    "grpc-web": "1.5.0",
    "reflect-metadata": "0.2.2",
    "rxjs": "7.8.1",
    "tslib": "2.8.1"
  },
  "engines": {
    "node": ">=10",
    "npm": ">=6.14.4"
  }
}
