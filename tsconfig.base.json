{
  "angularCompilerOptions": {
    "annotateForClosureCompiler": true,
    "enableIvy": true,
    "enableResourceInlining": true,
    "fullTemplateTypeCheck": true,
    "skipTemplateCodegen": true,
    "strictInjectionParameters": true,
    "strictMetadataEmit": true,
    "strictTemplates": true
  },
  "compileOnSave": false,
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "baseUrl": ".",
    "declaration": false,
    "downlevelIteration": true,
    "emitDecoratorMetadata": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "forceConsistentCasingInFileNames": true,
    "importHelpers": true,
    "lib": ["ES2018", "DOM", "ESNext.AsyncIterable"],
    "module": "ESNext",
    "moduleResolution": "Node",
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": true,
    "noImplicitReturns": false,
    "noUnusedLocals": true,
    "paths": {
      "@nx-ng-starter/api-interface": ["libs/api-interface/src/index.ts"],
      "@nx-ng-starter/api-interface/client-gql": ["libs/api-interface/src/lib/generated/index.ts"],
      "@nx-ng-starter/mocks-core": ["libs/mocks-core/src/index.ts"],
      "@nx-ng-starter/proto": ["libs/proto/src/index.ts"],
      "@nx-ng-starter/client-core": ["libs/client-core/src/index.ts"],
      "@nx-ng-starter/client-store": ["libs/client-store/src/index.ts"],
      "@nx-ng-starter/client-store/*": ["libs/client-store/src/lib/*"],
      "@nx-ng-starter/client-ui": ["libs/client-ui/src/index.ts"],
      "@nx-ng-starter/client-util": ["libs/client-util/src/index.ts"],
      "@nx-ng-starter/client-ui-material": ["libs/client-ui-material/src/index.ts"],
      "@nx-ng-starter/client-ui-translate": ["libs/client-ui-translate/src/index.ts"],
      "@nx-ng-starter/client-services": ["libs/client-services/src/index.ts"]
    },
    "rootDir": ".",
    "skipDefaultLibCheck": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": true,
    "suppressImplicitAnyIndexErrors": true,
    "target": "ES6",
    "typeRoots": ["node_modules/@types"]
  },
  "exclude": [
    "node_modules",
    "tmp",
    ".azure",
    ".devops",
    ".firebase",
    ".vscode",
    "coverage",
    "dist",
    "documentation",
    "functions",
    "libs/proto/src/lib/grpc",
    "libs/proto/src/lib/ts",
    "tools/proto",
    "tools/shell"
  ]
}
