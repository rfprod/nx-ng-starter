    "rootDir": ".",
    "skipDefaultLibCheck": true,
    "skipLibCheck": true,
    "strict": true,
    "target": "ES2022",
    "typeRoots": [
      "node_modules/@types"
    ],
    "useDefineForClassFields": false
  },
  "exclude": [
    "node_modules",
    "tmp",
    ".angular",
    ".docker",
    ".firebase",
    ".github",
    ".husky",
    ".storybook",
    ".vscode",
    "android",
    "changelog",
