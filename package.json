{
  "name": "nx-ng-starter",
  "version": "1.0.0",
  "private": true,
  "description": "NX Angular Starter",
  "license": "MIT",
  "scripts": {
    "affected": "nx affected",
    "affected:apps": "nx affected:apps",
    "affected:build": "nx affected:build",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected:e2e": "nx affected:e2e",
    "affected:libs": "nx affected:libs",
    "affected:lint": "nx affected:lint",
    "affected:test": "nx affected:test",
    "analyze:client": "npm run build:client:stats; npx webpack-bundle-analyzer ./dist/apps/client/stats.json",
    "build:all": "npm run build:api && npm run build:client && npm run build:documentaion",
    "build:all:prod": "bash tools/shell/build.sh prod all",
    "build:api": "ng build api",
    "build:api:firebase": "ng build api --configuration=firebase",
    "build:api:prod": "bash tools/shell/build.sh prod api",
    "build:client": "ng build client",
    "build:client:prod": "bash tools/shell/build.sh prod client",
    "build:client:stats": "ng build --stats-json --project client --configuration analyze-bundle",
    "build:documentation": "npm run generate:doc-env; ng build documentation; npm run document:all:generate-and-report-to-dist; npm run changelog:all:generate; npm run e2e:all:headless:report; npm run test:all:single-run-and-report-to-dist",
    "build:documentation:prod": "npm run generate:doc-env; bash tools/shell/build.sh prod documentation",
    "changelog:all:generate": "bash tools/shell/changelog.sh all",
    "dep-graph": "nx dep-graph",
    "document": "bash tools/shell/document.sh",
    "document:all:generate": "bash tools/shell/document.sh generate all",
    "document:all:generate-and-report-to-dist": "bash tools/shell/document.sh generate-and-report all",
    "e2e": "nx e2e",
    "e2e:all": "bash tools/shell/e2e.sh all",
    "e2e:all:headless": "bash tools/shell/e2e.sh all headless",
    "e2e:all:headless:report": "bash tools/shell/e2e.sh all headless report",
    "e2e:client": "bash tools/shell/e2e.sh app:client-e2e",
    "e2e:client:headless": "bash tools/shell/e2e.sh app:client-e2e headless",
    "e2e:client:headless:report": "bash tools/shell/e2e.sh app:client-e2e headless report",
    "e2e:client:report": "bash tools/shell/e2e.sh app:client-e2e browser report",
    "firebase:deploy:ci:all": "bash tools/shell/firebase-deploy.sh $FIREBASE_DEPLOY_TOKEN all",
    "firebase:deploy:ci:api": "bash tools/shell/firebase-deploy.sh $FIREBASE_DEPLOY_TOKEN app:api",
    "firebase:deploy:ci:client": "bash tools/shell/firebase-deploy.sh $FIREBASE_DEPLOY_TOKEN app:client",
    "firebase:deploy:ci:documentation": "bash tools/shell/firebase-deploy.sh $FIREBASE_DEPLOY_TOKEN app:documentation",
    "firebase:deploy:localhost:all": "npm run build:all:prod && bash tools/shell/firebase-deploy.sh localhost all",
    "firebase:deploy:localhost:api": "npm run build:api:firebase && bash tools/shell/firebase-deploy.sh localhost app:api",
    "firebase:deploy:localhost:client": "npm run build:client:prod && bash tools/shell/firebase-deploy.sh localhost app:client",
    "firebase:deploy:localhost:documentation": "npm run build:documentation:prod && bash tools/shell/firebase-deploy.sh localhost app:documentation",
    "format": "nx format:write",
    "format:check": "nx format:check",
    "format:write": "nx format:write",
    "generate:client-gql": "ng run api-interface:generate-client-definitions",
    "generate:doc-env": "bash tools/shell/set-documentation-env.sh",
    "generate:proto:all": "npx npm-run-all -s generate:protobufjs generate:protoc",
    "generate:protobufjs": "bash tools/shell/generate-proto.sh protobufjs",
    "generate:protoc": "bash tools/shell/generate-proto.sh protoc",
    "help": "nx help",
    "postinstall": "npx sort-package-json && npm run ngcc && node ./decorate-angular-cli.js",
    "install:all:linux": "bash tools/shell/install.sh all",
    "install:all:osx": "bash tools/shell/install.sh all osx",
    "install:global": "bash tools/shell/install.sh global",
    "install:project": "bash tools/shell/install.sh project",
    "install:proto:linux": "bash tools/shell/install.sh proto",
    "install:proto:osx": "bash tools/shell/install.sh proto osx",
    "install:shellcheck:linux": "bash tools/shell/install.sh shellcheck",
    "install:shellcheck:osx": "bash tools/shell/install.sh shellcheck osx",
    "lint": "npx nx workspace-lint && ng lint",
    "lint:all": "bash tools/shell/lint.sh all",
    "lint:all:fix": "bash tools/shell/lint.sh all fix",
    "lint:changed": "bash tools/shell/lint.sh changed",
    "lint:changed:fix": "bash tools/shell/lint.sh changed fix",
    "ng": "nx",
    "ng-build": "ng build",
    "ng-e2e": "ng e2e",
    "ngcc": "ngcc --tsconfig tsconfig.base.json",
    "nx": "nx",
    "nx:lint": "./node_modules/.bin/nx workspace-lint && ng lint",
    "nx:lint:fix": "./node_modules/.bin/nx workspace-lint && ng lint --fix",
    "prettier:all:html": "npm run prettier:apps:html && prettier:libs:html",
    "prettier:all:html:fix": "npm run prettier:apps:html:fix && prettier:libs:html:fix",
    "prettier:apps:html": "npx prettier -c apps/**/**/*.html",
    "prettier:apps:html:fix": "npx prettier -c --write apps/**/**/*.html",
    "prettier:libs:html": "npx prettier -c libs/**/**/*.html",
    "prettier:libs:html:fix": "npx prettier -c --write libs/**/**/*.html",
    "protolint": "protolint lint -config_path ./.protolint.yaml ./tools/proto/*",
    "shellcheck": "shellcheck **/*.sh",
    "start": "npm run start:api & npm run start:client",
    "start:api": "ng serve api",
    "start:api:local": "npm run start:envoy:local & ng serve api",
    "start:client": "ng serve client",
    "start:envoy": "sudo bash tools/shell/docker.sh envoy",
    "start:envoy:local": "sudo bash tools/shell/docker.sh envoy:local",
    "start:production-server": "node simple-production-server.js",
    "stylelint": "npx stylelint --syntax scss",
    "stylelint:all": "npm run stylelint:apps; npm run stylelint:libs",
    "stylelint:all:fix": "npm run stylelint:apps:fix; npm run stylelint:libs:fix",
    "stylelint:apps": "npx stylelint \"./apps/**/*.scss\"",
    "stylelint:apps:fix": "npx stylelint \"./apps/**/*.scss\" --fix",
    "stylelint:libs": "npx stylelint \"./libs/**/*.scss\"",
    "stylelint:libs:fix": "npx stylelint \"./libs/**/*.scss\" --fix",
    "test": "bash tools/shell/test.sh",
    "test:all:single-run": "bash tools/shell/test.sh single-run all",
    "test:all:single-run-and-report-to-dist": "bash tools/shell/test.sh single-run-and-report all",
    "test:changed:single-run": "bash tools/shell/test.sh single-run changed",
    "update": "ng update @nrwl/workspace @angular/cli @angular/core --force",
    "update:check": "ng update",
    "workspace-schematic": "nx workspace-schematic",
    "workspace:cleanup": "rm -rf documentation/* dist/* coverage/* changelog/*"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint:changed",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {
