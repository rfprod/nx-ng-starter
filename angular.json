{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "cli": {
    "defaultCollection": "@nrwl/angular",
    "warnings": {
      "versionMismatch": false
    }
  },
  "defaultProject": "client",
  "newProjectRoot": "",
  "projects": {
    "api": {
      "architect": {
        "build": {
          "builder": "@nrwl/node:build",
          "configurations": {
            "firebase": {
              "extractLicenses": true,
              "fileReplacements": [
                {
                  "replace": "apps/api/src/environments/environment.ts",
                  "with": "apps/api/src/environments/environment.firebase.ts"
                }
              ],
              "inspect": false,
              "optimization": true
            },
            "production": {
              "extractLicenses": true,
              "fileReplacements": [
                {
                  "replace": "apps/api/src/environments/environment.ts",
                  "with": "apps/api/src/environments/environment.prod.ts"
                }
              ],
              "inspect": false,
              "optimization": true
            }
          },
          "options": {
            "assets": ["apps/api/src/assets"],
            "buildLibsFromSource": true,
            "main": "apps/api/src/main.ts",
            "outputPath": "dist/apps/api",
            "tsConfig": "apps/api/tsconfig.app.json"
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "eslintConfig": "apps/api/.eslintrc",
            "exclude": ["**/node_modules/**", "!apps/api/**/*.ts"],
            "lintFilePatterns": ["apps/api/**/*.ts"]
          }
        },
        "serve": {
          "builder": "@nrwl/node:execute",
          "configurations": {
            "firebase": {
              "buildTarget": "api:build:firebase"
            },
            "production": {
              "buildTarget": "api:build:production"
            }
          },
          "options": {
            "buildTarget": "api:build"
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/api/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "prefix": "api",
      "projectType": "application",
      "root": "apps/api",
      "schematics": {},
      "sourceRoot": "apps/api/src"
    },
    "api-interface": {
      "architect": {
        "generate-client-definitions": {
          "builder": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              {
                "command": "npx graphql-codegen --config libs/api-interface/codegen.yml && find libs/client-gql/src/lib/generated/ -type f -name \"*.ts\" -exec sed -i \"s/Scalars/IScalars/g\" {} + && ng lint client-gql --fix"
              }
            ]
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "eslintConfig": "libs/api-interface/.eslintrc",
            "exclude": ["**/node_modules/**", "!libs/api-interface/**/*.ts"],
            "lintFilePatterns": ["libs/api-interface/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/api-interface/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "projectType": "library",
      "root": "libs/api-interface",
      "schematics": {},
      "sourceRoot": "libs/api-interface/src"
    },
    "client": {
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "configurations": {
            "analyze-bundle": {
              "aot": true,
              "budgets": [
                {
                  "maximumError": "5mb",
                  "maximumWarning": "2mb",
                  "type": "initial"
                },
                {
                  "maximumError": "10kb",
                  "maximumWarning": "6kb",
                  "type": "anyComponentStyle"
                }
              ],
              "buildOptimizer": true,
              "extractCss": true,
              "extractLicenses": true,
              "fileReplacements": [
                {
                  "replace": "apps/client/src/environments/environment.ts",
                  "with": "apps/client/src/environments/environment.prod.ts"
                }
              ],
              "namedChunks": true,
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": true,
              "vendorChunk": true
            },
            "production": {
              "aot": true,
              "budgets": [
                {
                  "maximumError": "5mb",
                  "maximumWarning": "2mb",
                  "type": "initial"
                },
                {
                  "maximumError": "10kb",
                  "maximumWarning": "6kb",
                  "type": "anyComponentStyle"
                }
              ],
              "buildOptimizer": true,
              "extractCss": true,
              "extractLicenses": true,
              "fileReplacements": [
                {
                  "replace": "apps/client/src/environments/environment.ts",
                  "with": "apps/client/src/environments/environment.prod.ts"
                }
              ],
              "namedChunks": true,
              "ngswConfigPath": "apps/client/ngsw-config.json",
              "optimization": true,
              "outputHashing": "all",
              "serviceWorker": true,
              "sourceMap": false,
              "vendorChunk": true
            }
          },
          "options": {
            "allowedCommonJsDependencies": [
              "node_modules/hammerjs/hammer.js",
              "google-protobuf/google/protobuf/any_pb.js",
              "protobufjs/minimal",
              "zen-observable",
              "apollo-upload-client",
              "extract-files/public/isExtractableFile",
              "extract-files/public/extractFiles",
              "graphql-tag",
              "subscriptions-transport-ws"
            ],
            "aot": true,
            "assets": [
              "apps/client/src/favicon.ico",
              "apps/client/src/assets",
              "apps/client/src/manifest.webmanifest",
              {
                "glob": "**/*",
                "input": "./libs/client-core/src/lib/assets",
                "output": "./assets"
              }
            ],
            "index": "apps/client/src/index.html",
            "main": "apps/client/src/main.ts",
            "outputPath": "dist/apps/client",
            "polyfills": "apps/client/src/polyfills.ts",
            "scripts": [],
            "styles": [
              "node_modules/roboto-fontface/css/roboto/roboto-fontface.css",
              "node_modules/material-design-icon-fonts/iconfont/material-icons.css",
              "apps/client/src/styles.scss",
              "libs/client-core/src/lib/styles/styles.scss"
            ],
            "tsConfig": "apps/client/tsconfig.app.json"
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "client:build"
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "eslintConfig": "apps/client/.eslintrc",
            "exclude": ["**/node_modules/**", "!apps/client/**/*.ts"],
            "lintFilePatterns": ["apps/client/**/*.ts"]
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "client:build:production"
            }
          },
          "options": {
            "browserTarget": "client:build",
            "proxyConfig": "apps/client/proxy.conf.json"
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "assets": [
              "apps/client/src/favicon.ico",
              "apps/client/src/assets",
              {
                "glob": "**/*",
                "input": "./libs/client-core/src/lib/assets",
                "output": "./assets"
              }
            ],
            "jestConfig": "apps/client/jest.config.js",
            "passWithNoTests": true,
            "scripts": [],
            "styles": [
              "node_modules/roboto-fontface/css/roboto/roboto-fontface.css",
              "node_modules/material-design-icon-fonts/iconfont/material-icons.css",
              "apps/client/src/styles.scss",
              "libs/client-core/src/lib/styles/styles.scss"
            ]
          }
        }
      },
      "prefix": "client",
      "projectType": "application",
      "root": "apps/client",
      "schematics": {
        "@nrwl/workspace:component": {
          "style": "scss"
        }
      },
      "sourceRoot": "apps/client/src"
    },
    "client-components": {
      "architect": {
        "build-storybook": {
          "builder": "@nrwl/storybook:build",
          "configurations": {
            "ci": {
              "quiet": true
            }
          },
          "options": {
            "config": {
              "configFolder": "libs/client-components/.storybook"
