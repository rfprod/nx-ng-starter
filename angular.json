        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "eslintConfig": "libs/client-components/.eslintrc",
            "exclude": ["**/node_modules/**", "!libs/client-components/**/*.ts"],
            "lintFilePatterns": ["libs/client-components/**/*.ts"]
          }
        },
        "storybook": {
          "builder": "@nrwl/storybook:storybook",
          "configurations": {
            "ci": {
              "quiet": true
            }
          },
          "options": {
            "config": {
              "configFolder": "libs/client-components/.storybook"
            },
            "port": 4400,
            "uiFramework": "@storybook/angular"
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/client-components/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "prefix": "nx-ng-starter",
      "projectType": "library",
      "root": "libs/client-components",
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      },
      "sourceRoot": "libs/client-components/src"
    },
    "client-components-e2e": {
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
