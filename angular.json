          "options": {
            "cypressConfig": "apps/client-components-e2e/cypress.json",
            "devServerTarget": "client-components:storybook",
            "tsConfig": "apps/client-components-e2e/tsconfig.e2e.json"
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
